// confetti animation
// links at bottom 


let x,y;
let r,g,b;


function setup() {
createCanvas(800,700)
  background(255);
}

function draw() {
  
   //createCanvas(700, 700);
   x=random(width)
  y=random(height)
  r=random(15,252)
  g=random(0,255)
  b=random(0,255)
 
  fill(r,g,b,65)
  noStroke()
  ellipse(x,y,30);
  
}

